.nowClassBox {
    font-size: 32rpx;
    color: #3D3B5E;
    margin-top: 56rpx;
    margin-bottom: 53rpx;
    text-align: center;
}
.teac_url {
    position: absolute;
    left: 40rpx;
    top: 50%;
    transform: translateY(-50%);
    width: 72rpx;
    height: 72rpx;
    border-radius: 100%;
}
.teac_box_right {
    width: 100%;
}
.teac_time {
    font-size: 28rpx;
    color: #3D3B5E;
    margin-top: 48rpx;
}
.bigBox {
    width: 100%;
    display: inline-block;
    margin-bottom: 32rpx;
    border-bottom: 1px solid #E5E5E9;
}
.classBoxRight {
    width: 100%;
    padding-right: 40rpx;
    box-sizing: border-box;
    display: inline-block;
    padding-left: 25%;
    position: relative;
}
.rollCall,.photo {
    display: inline-block;
    width: 140rpx;
    height: 54rpx;
    line-height: 54rpx;
    text-align: center;
    font-size: 24rpx;
    border-radius: 54rpx;
    background: #ffffff;
    color: #FF8814;
    border: 1px solid #FF8814;
}
.rollCall {
    color: #ffffff;
    background: #FF8814;
    margin: 30rpx 20rpx 40rpx 0;
}
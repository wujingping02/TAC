.val {
    width: 100%;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #333333;
    height: 80rpx;
    line-height: 80rpx;
    border: 2rpx solid #E5E5E9;
    border-radius: 12rpx;
    margin-top: 80rpx;
    margin-bottom: 40rpx;
    padding: 0 20rpx;
    position: relative;
}
.prompt {
    width: 100%;
    height: 80rpx;
    overflow: hidden;
}
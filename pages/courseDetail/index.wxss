
.box {
    background: #ffffff;
    padding-bottom: 40rpx;
}
.courseCardBox {
    width: 100%;
    padding: 0 40rpx;
    background: #ffffff;
    transform: translateY(-20rpx);
    border-radius: 100rpx 100rpx 0 0;
    display: inline-block;
    box-sizing: border-box;
}
.courseCard {
    background: #3FBAF1;
    width: 100%;
    border-radius: 20rpx;
    box-shadow: 1px 1px 3px rgba(0,0,0,0.1);
    display: inline-block;
    text-align: center;
}
.name {
    font-size: 44rpx;
    color: #FFFFFF;
    text-align: center;
    margin-top: 80rpx;
}
.age {
    display: inline-block;
    background: rgba(255,255,255,0.10);
    border-radius: 28rpx;
    border-radius: 28rpx;
    padding: 16rpx 31rpx;
    font-size: 24rpx;
    color: #FFFFFF;
    margin-top: 25rpx;
}
.address {
    font-size: 32rpx;
    color: #FFFFFF;
    line-height: 32rpx;
    margin-top: 47rpx;
}
.address::before {
    content: "";
    display: inline-block;
    width: 34rpx;
    height: 32rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
    transform: translate(-5px, 2px);
}
.phoneBox {
    margin-top: 40rpx;
    display: inline-block;
    width: 220rpx;
    padding: 15rpx;
    border-radius: 64rpx;
    border: 1px solid #ffffff;
    text-align: center;
    margin-bottom: 56rpx;
}
.phone {
    font-size: 24rpx;
    color: #FFFFFF;
}
.phone::before {
    content: "";
    display: inline-block;
    width: 30rpx;
    height: 32rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
    transform: translate(-2px, 4px);
}
.introduction {
    font-size: 24rpx;
    color: #3D3B5E;
    line-height: 20px; 
    margin-top: 16rpx;
    padding: 0 40rpx;
    width: 100%;
    word-wrap: break-word;
    box-sizing: border-box;
}
.orgPhotoBox {
    margin-top: 24rpx;
    padding: 0 40rpx;
}
.orgName {
    padding: 52rpx 0 10rpx 40rpx;
    font-size: 28rpx;
    color: #3D3B5E;
}
.erweimaBox {
    position: relative;
    display: inline-block;
    width: 100%;
}
.roundLeft,.roundRight {
    display: inline-block;
    width: 50rpx;
    height: 50rpx;
    border-radius: 100%;
    background: #ffffff;
}
.roundLeft {
    position: absolute;
    left: 0;
    top: 0;
    transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
}
.roundRight {
    position: absolute;
    right: 0;
    top: 0;
    transform: translateX(50%);
    -webkit-transform: translateX(50%);
}
.rountCenter {
    display: inline-block;
    width: 80%;
    height: 2px;
    border-top: 1px dashed #ffffff;
    margin: 0 auto;
}
.canvasBox {
    width: 200px;
    height: 200px;
    background: #ffffff;
    margin: 50rpx auto 0;
}
.canvas {
    transform: translate(10px, 10px);
    -webkit-transform: translate(10px, 10px);
    transform-origin: 0 0;
    -ms-transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
}
.ewmPrompt {
    font-size: 28rpx;
    color: #FFFFFF;
    margin: 12rpx auto 58rpx;
}
.lessonBigBox {
    width: 100%;
    padding: 0 40rpx;
    display: inline-block;
    box-sizing: border-box;
}
.lessonBox {
    background: #F9F9FA;
    box-shadow: 0 2px 4px 0 rgba(0,0,0,0.10);
    border-radius: 32rpx;
    border-radius: 32rpx;
    margin-top: 48rpx;
}
.lessonTop {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 24rpx;
    color: #9D9DAF;
}
.lessonTopLeft,.lessonTopRight {
    display: inline-block;
    text-align: center;
}
.lessonTopLeft {
    width: 40%;
}
.lessonTopRight {
    width: 60%;
}
.lessonDetail {
    width: 100%;
    padding: 0 56rpx;
    box-sizing: border-box;
    border-radius: 32rpx;
    background: #ffffff;
}
.lessonDetailBox {
    width: 100%;
    height: 120rpx;
    line-height: 120rpx;
    border-bottom: 1px solid #E5E5E9;
    font-size: 28rpx;
    color: #3D3B5E;
}
.lessonDetailBoxLeft {
    display: inline-block;
    text-align: left;
    width: 40%;
}
.lessonDetailBoxRight {
    display: inline-block;
    width: 60%;
    text-align: center;
}
.upDownIconBox {
    width: 100%;
    height: 62rpx;
    display: inline-block;
}
.upDownIcon {
    display: block;
    width: 36rpx;
    height: 16rpx;
    margin: 26rpx auto 0;
}
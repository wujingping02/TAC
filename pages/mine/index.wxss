.mine {
    min-height: 100vh;
    background: #f5f5f5;
}
.avatarName {
    font-size: 48rpx;
    color: #FF8100;
    margin-top: 30rpx;
}
.listBox {
    width: 100%;
    padding: 40rpx 40rpx 0 40rpx;
    display: inline-block;
    box-sizing: border-box;
}
.listBox>view {
    width: 100%;
    height: 116rpx;
    line-height: 116rpx;
    border-radius: 20rpx;
    margin-bottom: 40rpx;
    background-color: #ffffff;
    position: relative;
    box-shadow: 1px 1px 5px rgba(0,0,0,0.16);
}
.listBox>view>image {
    width: 36rpx;
    height: 36rpx;
    margin-left: 38rpx;
    margin-right: 32rpx;
    transform: translateY(8rpx);
}
.text {
    font-size: 28rpx;
    color: #3D3B5E;
}
.text:after {
    position: absolute;
    top: 38rpx;
    right: 20rpx;
    content: "";
    width: 32rpx;
    height: 40rpx;
    background-image: url(data:image/png;base64,);
    background-repeat:no-repeat;
    background-size: 100% 100%;
}


.box {
    width: 100vw;
    display: inline-block;
    box-sizing: border-box;
    margin-top: 20rpx;
    padding-top: 24rpx;
}
.lable {
    font-size: 24rpx;
    color: #000000;
    line-height: 24rpx;
}
.orgPhotoBox {
    margin-top: 24rpx;
}
.orgPhotoBox.round {
    width: 27.5vw;
    height: 27.5vw;
    border-radius: 100%;
    overflow: hidden;
    position: relative;
}
.changeAvatarText {
    background: rgba(33, 33, 33, 0.8);
    color: #fff;
    position: absolute;
    left: 50%;
    bottom: 0rpx;
    transform: translateX(-50%);
    font-size: 24rpx;
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    z-index: 1;
}